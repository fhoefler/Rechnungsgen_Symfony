{% extends 'base.html.twig' %}


{% block body %}
<div class="row">
    <div class="col-md-3 offset-md-3 kunde">Kunden:</div>
</div>



<!-- contact form start -->

{% for data in car_form %}


<div class="container">
    <div class="row pb-0 align-items-center justify-content-center">
        <div class="col-lg-6 col-md-6">
            <div class="contact-block mb-4 mb-lg-0">
                <h5>{{ data.name }}  <a class='fa fa-plus' style='font-size:18px; text-align:right; float:right;' href="/rechnungspositions/{{ data.id }}"></a></h5>
